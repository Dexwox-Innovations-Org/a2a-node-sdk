{
  "name": "@dexwox-labs/a2a-auth",
  "version": "0.1.0",
  "description": "Comprehensive authentication framework for Google's Agent-to-Agent (A2A) protocol - includes OAuth2 flows, JWT handling, and security schemes",
  "homepage": "https://dexwox-innovations-org.github.io/a2a-node-sdk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dexwox-Innovations-Org/a2a-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Dexwox-Innovations-Org/a2a-node-sdk/issues"
  },
  "keywords": [
    "a2a",
    "agent-to-agent",
    "google",
    "protocol",
    "typescript",
    "authentication",
    "oauth2",
    "jwt",
    "security",
    "auth",
    "bearer",
    "basic",
    "api-key"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@dexwox-labs/a2a-core": "0.1.0",
    "@node-oauth/oauth2-server": "^5.1.0",
    "jose": "^5.9.6",
    "crypto-js": "^4.2.0",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.17.46",
    "@vitest/coverage-v8": "^3.1.3",
    "typescript": "^5.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "license": "Apache-2.0"
}