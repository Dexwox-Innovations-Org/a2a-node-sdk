{
  "name": "@dexwox/a2a-client",
  "version": "0.1.0",
  "description": "A2A Node SDK - HTTP Client for Agent-to-Agent Protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
